<div class ="container"><h1>Login</h1></div>

<%= form_with :url => "/sessions" do |form| %>
<div class = "container">
<div class = "row">
  <p>
    <%= form.label "email" %>
    <%= form.text_field "email" %>
    <%= form.label "password" %>
    <%= form.password_field "password" %>
  </p>
</div>
</div>

<div class="container">
  <p><%= form.submit "Login" , class:"btn btn-primary"%></p>
</div>
<% end %>