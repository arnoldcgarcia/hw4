<div class="container"><h1>Places</h1></div>
<div class = "container">
<% if @current_user%>
  <p><a href="/places/new" class="btn btn-primary" role="button">New place</a></p>
<% end%>
</div>

<div class = "column container">
<ul class="list-unstyled">
  <% for place in @places %>
    <li><a class="link-primary" href="/places/<%= place["id"] %>" ><%= place["name"] %></a></li>
  <% end %>
</ul>
</div>

